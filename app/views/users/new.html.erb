<h1>Registeration</h1>

<%=form_for User.new do |f|%>
<div>
	<%= f.label :email%><br>
	<%= f.text_field :email%>
</div>
<div>
	<%= f.label :password%><br>
	<%= f.password_field :password%>
</div>
<div>
	<%= f.label :password_confirmation%><br>
	<%= f.password_field :password_confirmation%>
</div>
<%= f.submit%>
<% end %>